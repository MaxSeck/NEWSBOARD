<div class="container">
<div class ="header-container">
 <h1 class="header-h1"><strong>Leave a vote</strong></h1>
<%= simple_form_for([@post, @vote]) do |f| %>
  <%= f.input :rating, input_html: { min: '1', max: '5' } %>
  <%= f.input :comment %>
  <%= f.submit "Submit", class: "btn-flat"%>
<% end %>
</div>
</div>
